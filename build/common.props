<Project>
  <Import Project="version.props" />
  <PropertyGroup>
    <LangVersion>latest</LangVersion>
    <Authors>virtual</Authors>
    <Product>AutoS3</Product>
    <PackageProjectUrl>https://github.com/cocosip/AutoS3</PackageProjectUrl>
    <PackageLicense>https://github.com/cocosip/AutoS3/blob/master/LICENSE</PackageLicense>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/cocosip/AutoS3</RepositoryUrl>
    <Description>AWS S3 SDK Client Manager</Description>
    <PackageTags>Amazon,AWSSDK,KS3,S3</PackageTags>
	
	<!-- https://github.com/dotnet/sourcelink -->
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.1.0-beta-20204-02">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers</IncludeAssets>
    </PackageReference>
  </ItemGroup>

</Project>